                                {# <img src="/assets/images/logo/logo.png" alt="Université de La Réunion"> #}
                                <img src="/assets/images/logo/logo-ur-blue.png" alt="Université de La Réunion">
                            </a>
                        </div>
                        <div class="logo d-none logo-light">
                            <a href="/">
                                <img src="https://www.univ-reunion.fr/wp-content/themes/univ974/assets/logos/logo-ur-blanc.svg" alt="Université de La Réunion">
                            </a>
                        </div>
                    </div>
                </div>

                <div class="rbt-main-navigation d-none d-xl-block">
                    <nav class="mainmenu-nav">
                        <ul class="mainmenu">
                            <li class="has-menu-child-item position-static">
                                <a href="/">Accueil</a>
                            </li>
                            {# <li class="has-menu-child-item position-static">
                                <a href="#">Test</a>
                            </li>
                            <li class="has-menu-child-item position-static">
                                <a href="#">Test</a>
                            </li>
                            <li class="has-menu-child-item position-static">
                                <a href="#">Test</a>
                            </li> #}
                        </ul>
                    </nav>
                </div>

                <div class="header-right">

                    <!-- Navbar Icons -->
                    <ul class="quick-access">

                        <li class="account-access rbt-user-wrapper d-none d-xl-block">
                            <a href="/dashboard"><i class="feather-user"></i>Tableau de bord</a>

                            {% if userConnected %}
                            <div class="rbt-user-menu-list-wrapper">
                                <div class="inner">
                                    <div class="rbt-admin-profile">
                                        {# <div class="admin-thumbnail">
                                            <img src="/assets/images/team/avatar.jpg" alt="User Images">
                                        </div> #}
                                        <div class="admin-info">
                                            <span class="name">Samuel</span>
                                            {# <a class="rbt-btn-link color-primary" href="#">Test</a> #}
                                        </div>
                                    </div>
                                    <ul class="user-list-wrapper">
                                        <li>
                                            <a href="/dashboard">
                                                <i class="feather-home"></i>
                                                <span>Tableau de bord</span>
                                            </a>
                                        </li>
                                        <li>
                                            <a href="/dashboard/history">
                                                <i class="feather-clock"></i>
                                                <span>Historique</span>
                                            </a>
                                        </li>
                                        <li>
                                            <a href="/dashboard/create">
                                                <i class="feather-message-square"></i>
                                                <span>Nouvelle demande</span>
                                            </a>
                                        </li>
                                    </ul>
                                    {# <hr class="mt--10 mb--10">

                                    <ul class="user-list-wrapper">
                                        <li>
                                            <a href="#">
                                                <i class="feather-book-open"></i>
                                                <span>test</span>
                                            </a>
                                        </li>
                                    </ul> #}

